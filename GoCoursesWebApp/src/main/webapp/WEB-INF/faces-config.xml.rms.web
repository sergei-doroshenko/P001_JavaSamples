    <!-- ***************************************
         Application
         *************************************** -->
    <application>
        <variable-resolver>org.springframework.web.jsf.DelegatingVariableResolver</variable-resolver>
        <variable-resolver>com.groundside.jsf.securityresolver.SecurityVariableResolver</variable-resolver>
        <property-resolver>com.groundside.jsf.securityresolver.SecurityPropertyResolver</property-resolver>
        <el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
        
        <!--  Fixed ERMS-2301 (See description in CustomViewHandler class)-->
		<view-handler>com.sun.faces.application.view.CustomViewHandler</view-handler>
    </application>

    <!--
            ***************************************
            Converters
            ***************************************
        -->

    <converter>
        <converter-id>stringConverter</converter-id>
        <converter-class>com.exadel.rms.presentation.util.StringConverter</converter-class>
    </converter>

    <converter>
        <description>Converter to strip out html from input string</description>
        <converter-id>
            htmlTrimmingConverter
        </converter-id>
        <converter-class>
            com.exadel.rms.common.presentation.converter.HtmlTrimmingConverter
        </converter-class>
    </converter>

    <converter>
        <description>Date Converter aware of date pattern mask '__/__/____'.</description>
        <converter-id>
            maskAwareDateTimeConverter
        </converter-id>
        <converter-class>
            com.exadel.rms.common.presentation.converter.MaskAwareDateTimeConverter
        </converter-class>
    </converter>

    <converter>
        <converter-for-class>java.lang.Enum</converter-for-class>
        <converter-class>javax.faces.convert.EnumConverter</converter-class>
    </converter>

    <converter>
        <description>Convert DecryptedValue objects to String representation.</description>
        <converter-id>decryptedValueConverter</converter-id>
        <converter-class>com.exadel.rms.common.presentation.converter.DecryptedValueConverter</converter-class>
    </converter>

    <factory>
        <exception-handler-factory>
            com.exadel.rms.exceptionhandler.ExtendExceptionHandlerFactory
        </exception-handler-factory>
    </factory>
